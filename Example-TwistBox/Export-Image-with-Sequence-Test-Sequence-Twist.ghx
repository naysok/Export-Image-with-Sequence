                    <item name="CodeInput" type_name="gh_string" type_code="10">import rhinoscriptsyntax as rs
import Rhino
import scriptcontext as sc
import ghpythonlib.components as ghpc


sc.doc = ghdoc


if toggle:
    
    for i in xrange(len(breps)):
#    for i in xrange(90, 95):
    
        
        name_format = "%03d"%i
        export_name = path + str(name_format) + type
        
        
        sc.doc = Rhino.RhinoDoc.ActiveDoc

        brep = ghpc.ListItem(breps, i)
#        brep_obj = rs.coercebrep(brep)
        sc.doc.Objects.AddBrep(brep)
        
        
        
        query_cap = "-ViewCaptureToFile "+ \
            export_name + \
            " W=1920 H=1080 S=1 D=_No R=_No A=_No T=_Yes " +\
            " _Enter "
        
        
        query_del = " _SelAll Delete "
        
        query = query_cap + query_del                
        
        rs.Command(query)
#        print(query)
        
        sc.doc = ghdoc</item>
