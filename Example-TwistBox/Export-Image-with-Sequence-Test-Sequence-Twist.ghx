            rect_rot, v1 = ghpc.Rotate(rs.coercecurve(tmp_rect), math.radians(tmp_angle), rs.WorldXYPlane())
            #print(tmp_angle)
            #print(type(tmp_angle))
            tmp_angle -= j
            rects_rot.append(rect_rot)
        
        #print(rects_rot)
        #print("---")
        
        loft_obj = rs.AddLoftSrf(rects_rot, None, None, 1)
        rs.CapPlanarHoles(loft_obj)
        #print(loft_obj)
        #print(type(loft_obj))
        #print(len(loft_obj))
        #print("---")
        loft_objs.append(loft_obj[0])
    #print(len(loft_objs))
    return loft_objs


def merge():
    
    a = []
    
    Right = rotate_lofts_Right(step_angle)
    Left = rotate_lofts_Left(step_angle)
    
    Right_reverse = copy.copy(Right)
    Right_reverse.reverse()
    Left_reverse = copy.copy(Left)
    Left_reverse.reverse()
    
    for i in xrange(len(Left) -1):
        a.append(Left[i])
        
    for i in xrange(len(Left_reverse) -1):
