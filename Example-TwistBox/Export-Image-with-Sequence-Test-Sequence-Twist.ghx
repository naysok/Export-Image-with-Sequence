import ghpythonlib.components as ghpc

import copy
import math


debug = []

def rects():
    rects = []
    
    for i in xrange(xy_dom+1):
        pt = rs.coerce3dpoint(rs.AddPoint(xy_dom*0.5*-1, xy_dom*0.5*-1, i))
        plane = rs.MovePlane(rs.WorldXYPlane(), pt)
        rect = rs.AddRectangle(plane, xy_dom, xy_dom)
        rects.append(rect)
       
    return rects


def rotate_lofts_Right(angle):
    
    loft_objs = []
    loft_objs_objs = []
    
    rects_ = rects()
    
    for j in xrange(angle+1):        
        
        rects_rot = []
        tmp_angle = 0
        
        for i in xrange(len(rects_)):
            tmp_rect = rects_[i]
            #print(tmp_rect) # guid
#            rect_rot = rs.RotateObject(tmp_rect, (0,0,0), tmp_angle)
            rect_rot, v1 = ghpc.Rotate(rs.coercecurve(tmp_rect), math.radians(tmp_angle), rs.WorldXYPlane())
            #print(tmp_angle)
            #print(type(tmp_angle))
            tmp_angle += j
            rects_rot.append(rect_rot)
        
        #print(rects_rot)
        #print("---")
        
        loft_obj = rs.AddLoftSrf(rects_rot, None, None, 1)
        rs.CapPlanarHoles(loft_obj)
        #print(loft_obj)
        #print(type(loft_obj))
        #print(len(loft_obj))
        #print("---")
        loft_objs.append(loft_obj[0])
    #print(len(loft_objs))
    return loft_objs


def rotate_lofts_Left(angle):
    
    loft_objs = []
    loft_objs_objs = []
    
    rects_ = rects()
    
    for j in xrange(angle+1):        
        
        rects_rot = []
        tmp_angle = 0
        
        for i in xrange(len(rects_)):
            tmp_rect = rects_[i]
            #print(tmp_rect) # guid
